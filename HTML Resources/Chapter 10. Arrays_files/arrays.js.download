		'11'  : function(){noclass(); execute('11'); schedNext('6c');},

		'12'  : function(){noclass(); execute('12'); vars.pc=12; vars.list[vars.j-1]=vars.swap; schedNext('List'+(vars.j-1));},
		'13'  : function(){noclass(); execute('13'); schedNext('4c');},

		'14'  : function(){noclass(); execute('14'); schedNext('End');},
		'End' : function(){noclass(); reset();},

		'List'  : function(){for(var i=0;i<vars.list.length;++i){assign('List'+i,vars.list[i]);} schedNext('2');},
		'List0' : function(){assign('List0',vars.list[0]); schedNext(vars.pc+1);},
		'List1' : function(){assign('List1',vars.list[1]); schedNext(vars.pc+1);},
		'List2' : function(){assign('List2',vars.list[2]); schedNext(vars.pc+1);},
		'List3' : function(){assign('List3',vars.list[3]); schedNext(vars.pc+1);},
		'List4' : function(){assign('List4',vars.list[4]); schedNext(vars.pc+1);},
		'First' : function(){assign('First',vars.first); schedNext('3');},
		'Last'  : function(){assign('Last',vars.last); schedNext('4a');},
		'I'     : function(){assign('I',vars.i); schedNext('4b');},
		'Swap'  : function(){assign('Swap',vars.swap); schedNext('6a');},
		'J'     : function(){assign('J',vars.j); schedNext('6b');}
	},
	{
		pc : 0,
		list : [],
		first : 0,
		last : 0,
		i : 0,
		swap : 0,
		j : 0
	})
		]);
		act.prepare();
	}
};
